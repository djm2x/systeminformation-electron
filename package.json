{
  "name": "electron",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "pi": "electron-builder install-app-deps",
    "i-electron": "npm i electron --ia32 -D",
    "i": "npm i sqlite3 && npm i electron-builder && npm run postinstall",
    "mode-developpement": "------------------------------------------------------------------",
    "f": "cd angular && ng serve",
    "b": "set ELECTRON_ENV=0 && set ELECTRON_ENV_TEST=true && electron --require ts-node/register ./main.ts",
    "b-n": "set ELECTRON_ENV=0 && set ELECTRON_ENV_TEST=true && electron --require ts-node/register ./main.ts",
    "$postinstall": "bash ./patch.sh",
    "ets": "set ELECTRON_ENV=0 && electron main.js",
    "build-watch": "ng build --extractCss --watch --base-href ./",
    "electron:serve": "npm run ets && nodemon --watch ./main.ts --exec \"npm run ets\"",
    "s": "electron ./portable8-win32-x64/resources/app/main.js",
    "dev": "concurrently \"ng serve\" \"npm run ets\"",
    "mode-production": "---------------------------------------------------------------------",
    "angular-dist": "electron --require ts-node/register ./main.ts",
    "f-p": "cd angular && ng build --prod --base-href ./",
    "b-p": "tsc  --project ./tsconfig.electron.json",
    "test": "set ELECTRON_ENV=0 && set ELECTRON_ENV_TEST=true && electron ./dist/main.js",
    "p": "npm run f-p && npm run b-p",
    "build": "node --require ts-node/register builderAPI.ts",
    "pkg": "electron-packager electron-js portable5",
    "prod": "npm run p && npm run build",
    "git": "---------------------------------------------------------------------",
    "create-branch": "git branch new-one",
    "move-to-branch": "git checkout new-one",
    "move-to-master-branch": "git checkout",
    "merge-branch-to-master-branch": "git merge new-one",
    "init": "git init && git remote add origin https://github.com/djm2x/systeminformation-electron.git",
    "push": "git add . && git commit -m \"change name foler api to node\" && git push -u origin master",
    "t": "echo \"Error: no test specified\" && exit 1"
  },
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "electron-reloader": "^1.0.1",
    "fetch-installed-software": "0.0.6",
    "systeminformation": "^4.23.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "electron-builder": "^22.7.0",
    "@types/node": "^12.11.1",
    "electron": "^9.0.1",
    "ts-node": "~8.3.0",
    "typescript": "~3.7.5"
  }
}
