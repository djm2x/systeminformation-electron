{
  "name": "angular",
  "version": "0.0.0",
  "scripts": {
    "pi": "electron-builder install-app-deps",
    "i-electron": "npm i electron --ia32 -D",
    "i": "npm i sqlite3 && npm i electron-builder && npm run postinstall",
    "mode-developpement": "------------------------------------------------------------------",
    "b": "set ELECTRON_ENV=0 && electron --require ts-node/register ./index.ts",
    "$postinstall": "bash ./patch.sh",
    "ets": "set ELECTRON_ENV=0 && electron index.js",
    "f": "ng serve",
    "build-watch": "ng build --extractCss --watch --base-href ./",
    "electron:serve": "npm run ets && nodemon --watch ./index.ts --exec \"npm run ets\"",
    "s": "electron ./portable8-win32-x64/resources/app/index.js",
    "dev": "concurrently \"ng serve\" \"npm run ets\"",
    "mode-production": "---------------------------------------------------------------------",
    "angular-dist": "electron --require ts-node/register ./index.ts",
    "angular-prod": "ng build --prod --base-href ./",
    "electron-prod": "tsc  --project ./tsconfig.electron.json",
    "prod3": "npm run angular-prod && npm run electron-prod && electron ./electron/index.js",
    "prod-a-e": "npm run angular-prod && npm run electron-prod",
    "prod2": "npm run angular-prod && npm run electron-prod",
    "build": "node --require ts-node/register builderAPI.ts",
    "pkg": "electron-packager electron-js portable5",
    "prod": "npm run prod-a-e && npm run build",
    "git": "---------------------------------------------------------------------",
    "create-branch": "git branch new-one",
    "move-to-branch": "git checkout new-one",
    "move-to-master-branch": "git checkout",
    "merge-branch-to-master-branch": "git merge new-one",
    "init": "git init && git remote add origin https://github.com/djm2x/systeminformation-electron.git",
    "push": "git add . && git commit -m \"change name foler api to node\" && git push -u origin master",
    "t": "echo \"Error: no test specified\" && exit 1"
  },
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "systeminformation": "^4.23.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular/animations": "~9.0.2",
    "@angular/cdk": "^9.1.0",
    "@angular/cli": "~9.0.3",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/forms": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@angular/material": "^9.1.0",
    "@angular/material-moment-adapter": "^9.1.0",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/node": "^12.11.1",
    "bootstrap-css-only": "^4.4.1",
    "codelyzer": "^5.1.2",
    "electron": "^8.2.0",
    "electron-builder": "^22.4.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "moment": "^2.24.0",
    "ngx-toastr": "^12.0.0",
    "protractor": "~5.4.3",
    "rxjs": "~6.5.4",
    "ts-node": "~8.3.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "zone.js": "~0.10.2"
  }
}
